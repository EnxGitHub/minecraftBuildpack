{
  "name": "Minecraft",
  "description": "EnxGitHub Minecaft server",
  "keywords": [
    "games",
    "minecraft"
  ],
  "repository": "https://github.com/EnxGitHub/minecraftBuildpack",
  "env": {
    "MINECRAFT_EULA": {
      "description": "Do you accept the Minecraft EULA?",
      "value": "true"
    },
    "NGROK_API_TOKEN": {
      "description": "Your personal ngrok API token",
      "required": true
    },
    "DROPBOX_ACCESS_TOKEN": {
      "description": "The dropbox access token for syncing",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "heroku/jvm"
    },
    {
      "url": "heroku-community/inline"
    }
    
  ]
}
